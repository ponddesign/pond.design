{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "react-router build",
    "icons": "npx @svgr/cli --out-dir app/components/icons -- app/icons",
    "icons:watch": "npm-watch icons",
    "dev:svg": "run-s icons icons:watch",
    "dev:remix": "react-router dev",
    "dev": "run-p dev:*",
    "typecheck": "react-router typegen && tsc"
  },
  "watch": {
    "icons": {
      "patterns": [
        "icons"
      ],
      "extensions": "svg",
      "quiet": false
    }
  },
  "dependencies": {
    "@react-router/node": "7.13.0",
    "isbot": "^3.6.8",
    "react-router": "7.13.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@heroicons/react": "^2.0.18",
    "@react-router/dev": "7.13.0",
    "@react-router/fs-routes": "7.13.0",
    "@remix-run/eslint-config": "2.17.0",
    "@svgr/cli": "^8.0.1",
    "@svgr/plugin-jsx": "^8.0.1",
    "@svgr/plugin-prettier": "^8.0.1",
    "@svgr/plugin-svgo": "^8.0.1",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@types/node": "^22.10.0",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@vercel/analytics": "^1.0.1",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.9",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.10.0",
    "autoprefixer": "^10.4.21",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.11.0",
    "postcss": "^8.5.6",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.0.4",
    "usehooks-ts": "^2.9.1",
    "vite": "^7.1.4"
  },
  "engines": {
    "node": "22.x"
  },
  "volta": {
    "node": "22.22.0"
  }
}
